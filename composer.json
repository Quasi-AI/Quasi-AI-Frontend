{
    "name": "quasiai/frontend",
    "description": "Fake PHP project to trigger deployment",
    "type": "project",
    "require": {
        "php": ">=7.4"
    },
    "scripts": {
        "post-install-cmd": [
            "npm install -g yarn pm2",
            "yarn install",
            "yarn build",
            "pm2 start .output/server/index.mjs --name=quasiai-frontend || pm2 restart quasiai-frontend",
            "pm2 save"
        ]
    }
}
